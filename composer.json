{
    "name": "jamesbwi/blade-svg",
    "description": "Embed SVGs in Laravel Blade.",
    "keywords": [
        "blade-svg",
        "laravel",
        "blade",
        "svg",
        "blade component",
        "svg component"
    ],
    "homepage": "https://github.com/jamesbwi/blade-svg",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "James Smillie",
            "email": "james@wearebwi.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jamesbwi\\BladeSvg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jamesbwi\\BladeSvg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jamesbwi\\BladeSvg\\BladeSvgServiceProvider"
            ]
        }
    }
}
